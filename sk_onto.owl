<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://skalerakis.org/onto.owl"
         xmlns="http://skalerakis.org/onto.owl#">

<owl:Ontology rdf:about="http://skalerakis.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#observes">
  <rdfs:domain rdf:resource="#CelestialBody"/>
  <rdfs:range rdf:resource="#CelestialBody"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#locatedIn">
  <rdfs:domain rdf:resource="#CelestialBody"/>
  <rdfs:range rdf:resource="#Galaxy"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#NaturalSatellite">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#ArtificialSatellite">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:complementOf rdf:resource="#NaturalSatellite"/>
    </owl:Class>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Galaxy">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#CelestialBody">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#SpaceTelescope">
  <rdfs:subClassOf rdf:resource="#CelestialBody"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#CelestialBody"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#observes"/>
          <owl:someValuesFrom rdf:resource="#CelestialBody"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </rdfs:subClassOf>
</owl:Class>


</rdf:RDF>
