<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/onto.owl"
         xmlns="http://test.org/onto.owl#">

<owl:Ontology rdf:about="http://test.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#teaches">
  <rdfs:domain rdf:resource="#Professor"/>
  <rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#Person">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Professor">
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#Course">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#ActiveProfessor">
  <rdfs:subClassOf rdf:resource="#Professor"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Professor"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#teaches"/>
          <owl:someValuesFrom rdf:resource="#Course"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#InactiveProfessor">
  <rdfs:subClassOf rdf:resource="#Professor"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Professor"/>
        <owl:Class>
          <owl:complementOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#teaches"/>
              <owl:someValuesFrom rdf:resource="#Course"/>
            </owl:Restriction>
          </owl:complementOf>
        </owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Student">
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<Professor rdf:about="#bilas">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Professor>

<Student rdf:about="#giorgos">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Student>


</rdf:RDF>
